<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <servlet>
    <servlet-name>RegisterMember</servlet-name>
    <servlet-class>com.yz.furn.web.RegisterMemberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterMember</servlet-name>
    <url-pattern>/registerMember</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginMember</servlet-name>
    <servlet-class>com.yz.furn.web.LoginMemberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginMember</servlet-name>
    <url-pattern>/loginMember</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginAdmin</servlet-name>
    <servlet-class>com.yz.furn.web.LoginAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginAdmin</servlet-name>
    <url-pattern>/loginAdmin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ManageFurns</servlet-name>
    <servlet-class>com.yz.furn.web.ManageFurnsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManageFurns</servlet-name>
    <url-pattern>/manage/manageFurns</url-pattern>
  </servlet-mapping>
</web-app>